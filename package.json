{
  "name": "xorax-sdk",
  "version": "1.0.0",
  "description": "TypeScript SDK for Xorax privacy mixer on Solana",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "solana",
    "privacy",
    "mixer",
    "xorax",
    "blockchain",
    "cryptocurrency",
    "defi"
  ],
  "author": "Xorax Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/xorax/xorax-sdk"
  },
  "peerDependencies": {
    "@coral-xyz/anchor": "^0.30.0",
    "@solana/web3.js": "^1.95.0"
  },
  "devDependencies": {
    "@coral-xyz/anchor": "^0.30.1",
    "@solana/web3.js": "^1.95.8",
    "@types/node": "^20.10.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
